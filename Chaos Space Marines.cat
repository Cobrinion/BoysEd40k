          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="8e8d-6fd5-9ab4-0b55" type="max"/>
          </constraints>
          <rules>
            <rule id="07d4-75e3-96f7-affd" name="Stimulated by Pain" hidden="false">
              <description>The warlords of the Emperor’s Children thrive on pain. The more grievous their injuries, the deadlier they become.
Your Warlord&apos;s profile gains 1 Attack each time a wound is allocated to them.</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="e7de-8dcf-403a-b76a" value="15"/>
          </costs>
        </selectionEntry>
