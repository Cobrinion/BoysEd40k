          <characteristics>
            <characteristic name="Unit Type" typeId="2ba2-3a82-dfb7-7849">Demon Primarch, Flying Monsterous Creature</characteristic>
            <characteristic name="M" typeId="e8fa-a6b3-0e8c-18aa">9&quot;</characteristic>
            <characteristic name="WS" typeId="9660-0df5-9c5d-3604">9</characteristic>
