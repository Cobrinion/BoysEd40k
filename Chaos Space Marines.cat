          <characteristics>
            <characteristic name="Unit Type" typeId="2ba2-3a82-dfb7-7849">Primarch, Flying Monsterous Creature, Deamon</characteristic>
            <characteristic name="M" typeId="e8fa-a6b3-0e8c-18aa">8&quot;</characteristic>
