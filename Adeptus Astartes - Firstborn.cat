    <selectionEntryGroup id="4931-5179-77e7-06cf" name="Space Wolf Warlord Traits" hidden="false" collective="false" import="true">
      <constraints>
        <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="d3ba-b0f5-de47-3d85" type="max"/>
      </constraints>
      <selectionEntries>
        <selectionEntry id="8359-b34c-ab94-f7c1" name="Howl of the Death Wolf" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="15d2-73b7-3e4f-dcdf" type="max"/>
          </constraints>
          <rules>
            <rule id="96bd-2c79-da65-fcd8" name="Howl of the Wolf" hidden="false">
              <description>With a single howl from the Warlords lips, the Rout is spurred to action, taking the final killing sprint to deliver the death blow all to the echoing cry of his warriors. Once per game, a Warlord with this trait may declare it will howl. For the rest of that turn, all Space Wolves units in your army gain the benefits of the Fleet (2) special rule.</description>
            </rule>
          </rules>
          <infoLinks>
            <infoLink id="493b-86ca-8845-9a83" name="Fleet" hidden="false" targetId="b1ba-0709-4305-88e2" type="rule">
              <modifiers>
                <modifier type="set" field="name" value="Howl of the Wolf"/>
              </modifiers>
            </infoLink>
          </infoLinks>
          <costs>
