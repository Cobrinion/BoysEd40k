        <selectionEntry id="3934-57fd-919d-68cf" name="Show The Curse of the Wulfen Rules" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="0018-e48f-cf4f-3697" type="max"/>
